<!-- /views/users.pug -->
<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        title Users
    body
        a(href="/regist") Regist new member
        table
            thead
                tr
                    th Username
                    th Password
            tbody
                each user in users
                    tr
                        td= user.username
                        td= user.password
                
